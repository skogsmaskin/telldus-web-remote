{
  "name": "telldus-webremote",
  "version": "0.0.0",
  "description": "A web based control panel for telldus written in Node.js",
  "main": "_app.js",
  "author": "Bjørge Næss",
  "license": "MIT",
  "scripts": {
    "compile-static": "staticr --require babel-core/register public static-routes",
    "bloat": "staticr --stdout --route /browser.js static-routes/browserify-bundles.js | discify --open",
    "start": "TELLDUS=mock proxyreload . --port=3000 --throttle=150"
  },
  "bin": {
    "telldus-web-remote": "./bin/telldus-web-remote"
  },
  "dependencies": {
    "babel-core": "^5.0.9",
    "babelify": "^6.0.1",
    "bluebird": "^2.9.24",
    "body-parser": "^1.12.2",
    "browserify": "^9.0.7",
    "bundle-collapser": "^1.1.4",
    "classnames": "^1.2.0",
    "color": "^0.8.0",
    "debug": "^2.1.3",
    "defaults": "^1.0.2",
    "dom-event": "0.0.5",
    "envify": "^3.4.0",
    "error-capture-middleware": "0.0.2",
    "express": "^4.12.3",
    "fastclick": "^1.0.6",
    "he": "^0.5.0",
    "helmet": "^0.7.1",
    "isomorphic-fetch": "^2.0.0",
    "lodash.throttle": "^3.0.2",
    "node-fetch": "^1.0.6",
    "node-sass": "^3.0.0-alpha.0",
    "primus": "^3.0.2",
    "qs": "^2.4.1",
    "react": "^0.13.1",
    "react-spinner": "^0.2.1",
    "rebundler": "0.0.4",
    "rx": "^2.4.10",
    "rx-dom": "^5.0.3",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "spawn-stream": "^1.0.2",
    "staticr": "^2.1.2",
    "telldus": "0.0.9",
    "uglify-js": "^2.4.19",
    "whatwg-fetch": "^0.7.0",
    "ws": "^0.7.1",
    "xhr": "~2.0.1"
  },
  "devDependencies": {
    "livereactload": "^0.2.1",
    "proxyreload": "git://github.com/bjoerge/proxyreload",
    "quickreload": "^1.0.0-alpha.3"
  }
}

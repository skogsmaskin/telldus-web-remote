    "react-spinner": "^0.2.2",
    "rebundler": "0.2.0",
    "redux": "^3.0.2",
    "redux-thunk": "^1.0.0",
    "sass": "^0.5.0",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "spawn-stream": "^1.0.2",

{
  "name": "telldus-webremote",
  "version": "0.0.0",
  "description": "A web based control panel for telldus written in Node.js",
  "main": "_app.js",
  "author": "Bjørge Næss",
  "license": "MIT",
  "scripts": {
    "compile-static": "staticr --require ./.init.js --out-dir public static-routes/*",
    "lint": "eslint . --ext=.js,.jsx",
    "bloat": "staticr --require ./.init.js --stdout --route /browser.js static-routes/browserify-bundles.js | discify --open",
    "start": "remon --require ./.init.js ./bin/dev-server.js",
    "start-prod": "export NODE_ENV=production && npm run compile-static && npm start"
  },
  "bin": {
    "telldus-web-remote": "./bin/telldus-web-remote.js"
  },
  "dependencies": {
    "babel-plugin-lodash": "^2.2.1",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "body-parser": "^1.15.0",
    "bourbon": "^4.2.6",
    "browserify": "^13.0.0",
    "bundle-collapser": "^1.2.1",
    "classnames": "^2.2.3",
    "color": "^0.11.1",
    "cors": "^2.7.1",
    "debounce-promise": "^1.0.5",
    "debug": "^2.2.0",
    "defaults": "^1.0.3",
    "dom-event": "2.0.0",
    "domready": "^1.0.8",
    "envify": "^3.4.0",
    "error-capture-middleware": "0.0.2",
    "express": "^4.13.4",
    "fastclick": "^1.0.6",
    "he": "^0.5.0",
    "helmet": "^1.3.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.6.1",
    "node-fetch": "^1.4.1",
    "node-sass": "^3.4.2",
    "promise-latest": "^1.0.4",
    "qs": "^6.1.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-domcontentloaded": "^1.0.2",
    "react-redux": "^4.4.1",
    "react-spinner": "^0.2.3",
    "rebundler": "0.3.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "spawn-stream": "^1.0.2",
    "staticr": "^4.0.2",
    "telldus-ws": "github:bjoerge/telldus-ws",
    "uglify-js": "^2.6.2",
    "update-object": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "dev-error-handler": "^1.2.0",
    "disc": "^1.3.2",
    "eslint": "^2.4.0",
    "eslint-config-bengler": "^1.0.7",
    "eslint-plugin-import": "^1.2.0",
    "eslint-plugin-react": "^4.2.3",
    "estraverse-fb": "^1.3.1",
    "eventsource-polyfill": "^0.9.6",
    "quickreload": "^2.0.0",
    "redux-logger": "^2.6.1",
    "remon": "^1.0.2"
  }
}

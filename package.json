    "bundle-collapser": "^1.2.1",
    "classnames": "^2.2.0",
    "color": "^0.10.1",
    "cors": "^2.7.1",
    "debounce-promise": "^1.0.2",
    "debug": "^2.2.0",
    "defaults": "^1.0.3",
    "dom-event": "0.0.5",
    "domready": "^1.0.8",
    "envify": "^3.4.0",
    "error-capture-middleware": "0.0.2",
    "express": "^4.13.3",
    "fastclick": "^1.0.6",
    "he": "^0.5.0",
